{
  "name": "middle-office-frontend",
  "version": "1.0.0",
  "description": "Middle Office Trade Booking Simulation Frontend",
  "scripts": {
    "build": "npx tailwindcss -i ./public/styles/tailwind.css -o ./public/styles/output.css --minify",
